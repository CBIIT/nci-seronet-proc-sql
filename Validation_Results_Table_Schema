drop table if exists `test`.`Validation_Final_Results`;
CREATE TABLE IF NOT EXISTS `test`.`Validation_Final_Results` (
	`CBC_Name` varchar(255) not null,
    `Submission_Name` varchar(255) not null,
	`Submission_Date` datetime not null,
    `Submission_Errors` varchar(3),
    `Prior_Test_Results_Errors` int,
    `Demographic_Data_Errors` int,
    `Biospecimen_Data_Errors` int,
    `Aliquot_Data_Errors` int,
    `Equipment_Data_Errors` int,
    `Reagent_Data_Errors` int,
    `Consumable_Data_Errors` int,
     PRIMARY KEY (`CBC_Name`,`Submission_Name`,`Submission_Date`));
